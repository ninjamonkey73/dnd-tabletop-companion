{
  "name": "dnd-tabletop-companion",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --localize",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "move-en": "move .\\en-US\\* .\\ && rmdir .\\en-US",
    "deploy": "npm run build:prod && git checkout gh-pages && npx rimraf ./browser && npx cpy-cli \"dist/dnd-tabletop-companion/browser/**\" . && npm run move-en && git add . && git commit -m 'Deploy' && git push origin gh-pages && git checkout main",
    "open": "ng serve --open"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.9",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.9",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.14",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.0",
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/localize": "^19.2.14",
    "@types/jest": "^30.0.0",
    "cpy": "^11.1.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.6.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "typescript": "~5.7.2"
  }
}
